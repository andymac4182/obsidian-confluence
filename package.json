    "markdown-it-wikilinks": "^1.2.0",
    "mermaid": "^10.1.0",
    "mime-types": "^2.1.35",
    "prosemirror-markdown": "^1.10.1",
    "prosemirror-model": "1.14.3",
    "punycode": "^1.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl-next": "npm:react-intl@^5.18.1",
    "spark-md5": "^3.0.2",
    "stringify-object": "^5.0.0",
    "uuid": "^9.0.0"
