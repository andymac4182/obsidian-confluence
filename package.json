{
  "name": "obsidian-confluence",
  "version": "2.0.1",
  "description": "This plugin allows you to publish your notes to Confluence",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "devcli": "node esbuild-cli.config.mjs && node ./dist-cli/cli.js",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "fmt": "npx prettier --write src/",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts src/",
    "prettier-check": "npx prettier --check src/",
    "test": "jest"
  },
  "keywords": [],
  "author": "andymac4182",
  "license": "Apache 2.0",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/markdown-it": "^12.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.6",
    "@types/prosemirror-model": "^1.16.2",
    "@types/react-dom": "^18.0.11",
    "@types/spark-md5": "^3.0.2",
    "@types/stringify-object": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.58.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.16",
    "esbuild-jest-transform": "1.1.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "obsidian": "1.1.1",
    "prettier": "2.7.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@atlaskit/adf-utils": "^17.1.4",
    "@atlaskit/editor-common": "^72.4.0",
    "@atlaskit/editor-confluence-transformer": "^8.1.33",
    "@atlaskit/editor-json-transformer": "^8.8.3",
    "@atlaskit/renderer": "^107.3.2",
    "@electron/remote": "^2.0.9",
    "@types/prosemirror-state": "^1.3.0",
    "@types/prosemirror-transform": "^1.4.2",
    "@types/prosemirror-view": "^1.23.1",
