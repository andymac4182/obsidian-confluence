{
	"name": "obsidian-confluence",
	"version": "0.0.0",
	"description": "This plugin allows you to publish your notes to Confluence",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"fmt": "npx prettier --write src/",
		"prepare": "husky install"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/mime-types": "^2.1.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"markdown-it": "^13.0.1",
		"obsidian": "latest",
		"prettier": "2.7.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@atlaskit/adf-utils": "^17.1.4",
		"@atlaskit/editor-json-transformer": "^8.8.1",
		"@atlaskit/editor-markdown-transformer": "^4.2.16",
		"adf-builder": "^3.3.0",
		"confluence.js": "^1.6.1",
		"md-to-adf-dailykos": "^0.1.1",
		"mime-types": "^2.1.35",
		"prosemirror-model": "^1.18.1",
		"showdown-confluence": "^2.0.2"
	},
	"lint-staged": {
		"src/**/*": "prettier --write --ignore-unknown"
	}
}
